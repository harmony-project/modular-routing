{
  "name": "harmony-project/modular-routing",
  "type": "library",
  "description": "An implementation of the Symfony router using a customizable matching process",
  "keywords": ["routing", "modular", "database"],
  "homepage": "http://harmony-project.io",
  "license": "MIT",
  "support": {
    "docs": "http://harmony-project.io/docs/modular-routing",
    "issues": "https://github.com/harmony-project/modular-routing/issues"
  },
  "authors": [
    {
      "name": "Tim Goudriaan",
      "homepage": "http://codedmonkey.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/log": "^1.0",
    "symfony/config": "^3.4|^4.0",
    "symfony/event-dispatcher": "^3.4|^4.0",
    "symfony/http-foundation": "^3.4|^4.0",
    "symfony/routing": "^3.4|^4.0",
    "symfony/yaml": "^3.4|^4.0",
    "symfony-cmf/routing": "^2.0",
    "twig/twig": "^2.0"
  },
  "require-dev": {
    "doctrine/common": "^2.5",
    "doctrine/orm": "^2.5",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Harmony\\Component\\ModularRouting\\": "source/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
